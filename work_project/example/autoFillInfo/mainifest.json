{
  "manifest_version": 2,
  "name": "自动登录",
  "version": "1.0",
  "description": "暂无描述",
  "icons": {
      "16": "images/app.png",
      "48": "images/app.png",
      "128": "images/app.png"
  },
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_popup": "index.html"
  },
  "content_scripts": [
      {
          "matches": ["https://www.new-emu.cn/home/*"],
          "js": ["script.js"],
          "run_at": "document_start"
      }
  ],
  "homepage_url": "https://www.baidu.com",
  "permissions": [
      "contextMenus"
  ]
}